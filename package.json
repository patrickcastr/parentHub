{
  "name": "parenthub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:web": "vite",
    "dev:api": "nodemon --watch server --ext ts --exec ts-node --project tsconfig.server.json server/index.ts",
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:web\"",
    "db:gen": "dotenv -e .env -- npx prisma generate --schema server/prisma/schema.prisma",
    "db:migrate": "dotenv -e .env -- npx prisma migrate dev --schema server/prisma/schema.prisma",
    "db:studio": "dotenv -e .env -- npx prisma studio --schema server/prisma/schema.prisma",
    "db:push": "dotenv -e .env -- npx prisma db push --schema server/prisma/schema.prisma",
    "seed:auth-users": "dotenv -e .env -- ts-node --transpile-only --project tsconfig.server.json server/prisma/seed-auth-users.ts",
    "build": "tsc -b && vite build",
    "build:web": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier -w .",
    "test": "vitest"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/identity": "^4.11.1",
    "@azure/msal-browser": "^4.21.1",
    "@azure/msal-react": "^3.0.19",
    "@azure/storage-blob": "^12.28.0",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.5.1",
    "hcaptcha": "^0.2.0",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "lucide-react": "^0.542.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.9.4",
    "papaparse": "^5.4.1",
    "pino": "^9.9.1",
    "pino-pretty": "^13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.26.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "prisma": "^6.15.0",
    "sonner": "^2.0.7",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "vite": "^5.4.8",
    "vitest": "^1.6.0"
  }
}
